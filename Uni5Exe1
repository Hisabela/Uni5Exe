
public class Uni5Exe1 {

    public static void main(String[] args) {

        for (int par = 0; par <= 20; par += 2) {
            
            System.out.println("PAR: ");
            System.out.println(par);

        }
        for (int impar = 1; impar <= 20; impar += 2) {

            System.out.println("ÍMPAR: ");
            System.out.println(impar);
        }

    }

}
